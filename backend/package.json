{
    "name": "@AgentChain/backend",
    "version": "1.0.0",
    "description": "AgentChain blockchain backend with AI validator consensus",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:unit": "jest --testPathPattern=unit",
        "test:integration": "jest --testPathPattern=integration",
        "test:e2e": "jest --testPathPattern=e2e",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix",
        "type-check": "tsc --noEmit",
        "migrate": "node dist/database/migrate.js",
        "migrate:rollback": "node dist/database/migrate.js --rollback",
        "migrate:create": "node scripts/create-migration.js",
        "seed": "node dist/database/seed.js",
        "generate-genesis": "tsx scripts/generate-genesis.ts"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.24.3",
        "@ethereum/common": "^4.0.0",
        "@ethereum/tx": "^5.0.0",
        "@ethereum/util": "^9.0.0",
        "@ethereum/vm": "^7.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "joi": "^17.11.0",
        "libp2p": "^0.46.21",
        "node-schedule": "^2.1.1",
        "openai": "^4.20.1",
        "pg": "^8.11.3",
        "redis": "^4.6.10",
        "solc": "^0.8.21",
        "uuid": "^9.0.1",
        "web3": "^4.2.2",
        "winston": "^3.11.0",
        "ws": "^8.14.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.8",
        "@types/node": "^20.9.0",
        "@types/pg": "^8.10.7",
        "@types/uuid": "^9.0.7",
        "@types/ws": "^8.5.9",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "@typescript-eslint/parser": "^6.12.0",
        "eslint": "^8.54.0",
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "tsx": "^4.4.0",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/your-org/AgentChain.git",
        "directory": "backend"
    },
    "keywords": [
        "blockchain",
        "bsc",
        "ethereum",
        "ai",
        "consensus",
        "validators",
        "defi",
        "smart-contracts"
    ],
    "author": "AgentChain Contributors",
    "license": "MIT"
}